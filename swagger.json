{
  "openapi": "3.0.1",
  "info": {
    "title": "Webdev Hackation API",
    "version": "0.0.1"
  },
  "servers": [
    {
      "url": "http://localhost:3000/",
      "description": "Localhost server"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "paths": {
    "/api/v1/user/auth/login": {
      "post": {
        "tags": ["user"],
        "summary": "User login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string"
                    },
                    "user": {
                      "type": "object",
                      "properties": {
                        "email": {
                          "type": "string"
                        },
                        "fullname": {
                          "type": "string"
                        },
                        "school": {
                          "type": "string"
                        },
                        "major": {
                          "type": "string"
                        },
                        "student_id": {
                          "type": "string"
                        },
                        "phone_number": {
                          "type": "string"
                        },
                        "facebook": {
                          "type": "string"
                        },
                        "short_introduction": {
                          "type": "string"
                        },
                        "personal_registration": {
                          "type": "boolean"
                        },
                        "user_code": {
                          "type": "string"
                        },
                        "fee_status": {
                          "type": "boolean"
                        },
                        "team_id": {
                          "type": "string"
                        },
                        "note_by_admin": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Wrong username or password"
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/v1/user/auth/sign-up": {
      "post": {
        "tags": ["user"],
        "summary": "User sign up",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "fullname": {
                    "type": "string"
                  },
                  "school": {
                    "type": "string"
                  },
                  "major": {
                    "type": "string"
                  },
                  "student_id": {
                    "type": "string"
                  },
                  "phone_number": {
                    "type": "string"
                  },
                  "facebook": {
                    "type": "string"
                  },
                  "short_introduction": {
                    "type": "string"
                  },
                  "personal_registration": {
                    "type": "boolean"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Sign up successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string"
                    },
                    "user": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/v1/user": {
      "get": {
        "tags": ["user"],
        "summary": "Get user info",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Get successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string"
                    },
                    "user": {
                      "type": "object",
                      "properties": {
                        "email": {
                          "type": "string"
                        },
                        "fullname": {
                          "type": "string"
                        },
                        "school": {
                          "type": "string"
                        },
                        "major": {
                          "type": "string"
                        },
                        "student_id": {
                          "type": "string"
                        },
                        "phone_number": {
                          "type": "string"
                        },
                        "facebook": {
                          "type": "string"
                        },
                        "short_introduction": {
                          "type": "string"
                        },
                        "personal_registration": {
                          "type": "boolean"
                        },
                        "user_code": {
                          "type": "string"
                        },
                        "fee_status": {
                          "type": "boolean"
                        },
                        "team_id": {
                          "type": "string"
                        },
                        "note_by_admin": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/v1/admin/auth/login": {
      "post": {
        "tags": ["admin"],
        "summary": "Admin login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Wrong username or password"
          },
          "400": {
            "description": "Bad request"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/api/v1/admin/users": {
      "get": {
        "tags": ["admin"],
        "summary": "Get all users",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Get successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "email": {
                        "type": "string"
                      },
                      "fullname": {
                        "type": "string"
                      },
                      "school": {
                        "type": "string"
                      },
                      "major": {
                        "type": "string"
                      },
                      "student_id": {
                        "type": "string"
                      },
                      "phone_number": {
                        "type": "string"
                      },
                      "facebook": {
                        "type": "string"
                      },
                      "short_introduction": {
                        "type": "string"
                      },
                      "personal_registration": {
                        "type": "boolean"
                      },
                      "user_code": {
                        "type": "string"
                      },
                      "fee_status": {
                        "type": "boolean"
                      },
                      "team_id": {
                        "type": "string"
                      },
                      "note_by_admin": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    }
  }
}
